{
  "_schema_version": "1.0",
  "_description": "Unit metadata for anaerobic digester simulation results",
  "_verified_date": "2025-01-22",
  "_source": "QSDsan v1.4.2 + Codex investigation",
  "time_series": {
    "time": {
      "units": "days",
      "description": "Simulation time points"
    },
    "effluent_cod": {
      "units": "mg/L",
      "description": "COD trajectory (VERIFIED: scope.record stores mg/L)"
    },
    "effluent_vfa": {
      "units": "mg/L",
      "description": "VFA (S_ac) trajectory (VERIFIED: scope.record stores mg/L)"
    },
    "effluent_biomass": {
      "units": "mg/L",
      "description": "Biomass (X_ac) trajectory (VERIFIED: scope.record stores mg/L)"
    },
    "biogas_ch4": {
      "units": "mg/L",
      "description": "Methane concentration in gas phase"
    }
  },
  "streams": {
    "flow": {
      "units": "m3/d",
      "description": "Volumetric flow rate"
    },
    "temperature": {
      "units": "K",
      "description": "Stream temperature"
    },
    "pH": {
      "units": "dimensionless",
      "description": "pH value (log scale)"
    },
    "COD": {
      "units": "mg/L",
      "description": "Chemical oxygen demand"
    },
    "TSS": {
      "units": "mg/L",
      "description": "Total suspended solids"
    },
    "VSS": {
      "units": "mg/L",
      "description": "Volatile suspended solids"
    },
    "TKN": {
      "units": "mg-N/L",
      "description": "Total Kjeldahl nitrogen"
    },
    "TP": {
      "units": "mg-P/L",
      "description": "Total phosphorus"
    },
    "alkalinity": {
      "units": "meq/L",
      "description": "Alkalinity as CaCO3 equivalent"
    },
    "components": {
      "units": "mg/L",
      "description": "Individual mADM1 component concentrations"
    }
  },
  "biogas": {
    "flow_total": {
      "units": "m3/d",
      "description": "Total biogas production rate"
    },
    "methane_flow": {
      "units": "m3/d",
      "description": "Methane production rate"
    },
    "methane_flow_kg": {
      "units": "kg/d",
      "description": "Methane mass production rate"
    },
    "methane_percent": {
      "units": "%",
      "description": "Methane percentage in biogas"
    },
    "co2_percent": {
      "units": "%",
      "description": "CO2 percentage in biogas"
    },
    "h2s_ppm": {
      "units": "ppm",
      "description": "H2S concentration (parts per million)"
    }
  },
  "yields": {
    "COD_removal_efficiency": {
      "units": "%",
      "description": "COD removal percentage"
    },
    "VSS_destruction": {
      "units": "%",
      "description": "VSS destruction percentage"
    },
    "methane_yield_per_cod": {
      "units": "m3 CH4/kg COD removed",
      "description": "Specific methane yield"
    },
    "biomass_yield": {
      "units": "kg VSS/kg COD",
      "description": "Biomass yield coefficient"
    }
  },
  "sulfur": {
    "sulfate_removal": {
      "units": "%",
      "description": "Sulfate reduction percentage"
    },
    "h2s_production": {
      "units": "mg S/L",
      "description": "Sulfide production"
    },
    "srb_biomass": {
      "units": "mg COD/L",
      "description": "Sulfate-reducing bacteria concentration"
    }
  },
  "convergence": {
    "converged_at_days": {
      "units": "days",
      "description": "Time at which steady-state was reached"
    },
    "status": {
      "units": "text",
      "description": "Convergence status message"
    },
    "runtime_seconds": {
      "units": "s",
      "description": "Computational runtime"
    }
  }
}